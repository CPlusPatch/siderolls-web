{
    "extends": "./.wxt/tsconfig.json",
    "compilerOptions": {
        "allowImportingTsExtensions": true,
        "jsx": "react-jsx",
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "strictFunctionTypes": true,
        "verbatimModuleSyntax": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    }
}
